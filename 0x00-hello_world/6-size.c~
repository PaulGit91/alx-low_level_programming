#include <stdio.h>
/**
 * main - ENTRY
 * Return:Always 0
 */
int main(void)
{
  char c;
  int i;
  long li;
  long long lli;
  float f;

  printf("Size of a char: %li.\n", sizeof(c));
  printf("Size of an int: %li.\n", sizeof(i));
  printf("Size of a long int: %li.\n", sizeof(li));
  printf("Size of a long long int: %li.\n", sizeof(lli));
  printf("Size of a float: %li.\n", sizeof(f));
  return (0);
}
